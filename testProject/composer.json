{
    "name": "cyclonedx/demo-cyclonedx-php-composer",
    "description": "See cyclonedx/cyclonedx-php-composer in action. Therefore some example-requirements are added.",
    "type": "project",
    "license": "Apache-2.0",
    "config": {
        "lock": true,
        "optimize-autoloader": false,
        "classmap-authoritative": false,
        "sort-packages": false
    },
    "repositories": [
        {
            "type": "path",
            "url": "../",
            "symlink": false
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "cyclonedx/cyclonedx-php-composer": "*",
        "pear/mail_mime": "^1.10"
    }
}
