{
    "name": "tools/composer-normalize",
    "type": "metapackage",
    "description": "composer-normalize",
    "require": {
        "php": ">=7.4"
    },
    "require-dev": {
        "ergebnis/composer-normalize": "2.23.1",
        "roave/security-advisories": "dev-latest"
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "ergebnis/composer-normalize": true
        }
    },
    "prefer-stable": true
}
