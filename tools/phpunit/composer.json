{
    "name": "tools/phpunit",
    "description": "phpunit",
    "type": "metapackage",
    "require": {
        "php": "^8.1.31"
    },
    "require-dev": {
        "phpunit/phpunit": "10.5.45||11.5.7||12.0.2",
        "roave/security-advisories": "dev-latest"
    },
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": false
        },
        "preferred-install": "dist",
        "sort-packages": true
    }
}
