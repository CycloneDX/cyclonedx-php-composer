{
    "name": "cyclonedx/cyclonedx-php-composer-demo",
    "description": "demo of cyclonedx/cyclonedx-php-composer with a local dependency",
    "type": "project",
    "config": {
        "sort-packages": true,
        "preferred-install": "dist"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "path",
            "url": "packages/*",
            "symlink": true
        },
        {
            "type": "path",
            "url": "%cyclonedx-php-composer_project_path%",
            "symlink": true
        }
    ],
    "require": {
        "cyclonedx/cyclonedx-php-composer-local-demo-dependency": "@dev"
    },
    "require-dev": {
        "cyclonedx/cyclonedx-php-composer": "@dev"
    }
}
