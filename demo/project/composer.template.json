{
    "name": "cyclonedx/cyclonedx-php-composer-demo",
    "description": "demo of cyclonedx/cyclonedx-php-composer",
    "type": "project",
    "config": {
        "sort-packages": true,
        "preferred-install": "dist"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "path",
            "url": "%cyclonedx-php-composer_project_path%",
            "symlink": true
        }
    ],
    "require": {
        "symfony/symfony": "@stable"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "cyclonedx/cyclonedx-php-composer": "*"
    }
}
